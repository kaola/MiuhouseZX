<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--<item android:state_enabled="false" android:drawable=""/>-->

    <item android:state_enabled="false">
        <shape>
            <corners android:radius="5dp"/>
            <solid android:color="@color/btn_enabled_false2"/>
            <stroke android:color="@color/umeng_socialize_divider" android:width="0.5dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="5dp"/>
            <solid android:color="@color/bg_green_pressed"/>
            <stroke android:color="@color/umeng_socialize_divider" android:width="0.5dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="5dp"/>
            <solid android:color="@color/bg_green"/>
            <stroke android:color="@color/umeng_socialize_divider" android:width="0.5dp"/>
        </shape>
    </item>

</selector>